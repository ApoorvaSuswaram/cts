
	import static org.junit.Assert.*;    
	import org.junit.Test;  
	public class Testlogic {
	  
	    @Test  
	    public void testFindMax(){  
	        assertEquals(4,CalculateJunit.findMax(new int[]{1,3,4,2}));  
	        assertEquals(-1,CalculateJunit.findMax(new int[]{-12,-1,-3,-4,-2}));  
	    }  
	}  
