package com;
import java.io.FileOutputStream;
public class FileIO {

	public static void main(String[] args) {
		try
		{
			FileOutputStream fout=new FileOutputStream("E:\\cts\\add.java");
			fout.write(65);
			fout.close();
			   System.out.println("success...");    
        }catch(Exception e){System.out.println(e);}  
			
		}
	}

