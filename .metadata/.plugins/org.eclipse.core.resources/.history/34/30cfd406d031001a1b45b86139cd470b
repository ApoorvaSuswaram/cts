package com;
class A
{
	void display()
	{
		System.out.println("class A");
	}
}
class A1 extends A
{
	@Override
	void display() {
		// TODO Auto-generated method stub
		super.display();
	}
}


public class Finalexample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        A c=new A();
		System.out.println(c.display());
	}

}
